# get builds
REACT_APP_BUILD_ENDPOINT= /build/

# daily report overview
REACT_APP_DAILY_REPORT_OVERVIEW_ENDPOINT = /build/daily/?type=overview

# daily report by date stats
REACT_APP_DAILY_REPORT_BY_DATE_ENDPOINT = /build/daily/?type=fordate&date=

# daily report fault code date wise stats
REACT_APP_DAILY_REPORT_BY_FAULT_CODE_BY_DATEWISE = /build/daily/?type=datewise_fault_code_stats&date=

# all ocp_build_data branches
REACT_APP_OCP_BUILD_DATA_OPENSHIFT_BRANCH_ENDPOINT = /release/branch?type=all

# github developer apis stats
REACT_APP_GIT_HUB_RATE_LIMIT_ENDPOINT = /release/gitstats

# get autocomplete data
REACT_APP_AUTOCOMPLETE_FOR_NVR = /autocomplete/?type=dg_name


# get build data for a date from mysql
REACT_APP_DAILY_BUILD_DATA_FOR_A_DATE = /build/build_records/?type=all&date=

# get build data for a particual label from mysql
REACT_APP_DAILY_BUILD_DATA_FOR_A_COLUMN_AND_DATE = /build/build_records/?type=column_search&name={0}&value={1}&date={2}

# get current advisories id for a ocp-build-data branch
REACT_APP_CURRENT_ADVISORY_IDS_FOR_A_BRANCH_OCP_BUILD_DATA = /release/branch?type=openshift_branch_advisory_ids&branch=

# get advisory details for an advisory id
REACT_APP_ADVISORY_DETAILS_FOR_AN_ADVISORY_ID = /errata/advisory/?type=advisory&id=

# get errata user details
REACT_APP_USER_DETAILS_FOR_A_USER_ID = /errata/user?type=user&id=

# get incidents
REACT_APP_REPORTED_INCIDENTS = /incident/

REACT_APP_BREW_TASK_LINK= https://brewweb.engineering.redhat.com/brew/taskinfo?taskID=
REACT_APP_CGIT_BUILD_TABLE_LINK=http://pkgs.devel.redhat.com/cgit/containers/metrics-hawkular-openshift-agent/tree/?id=
REACT_APP_BREW_BUILD_LINK= https://brewweb.engineering.redhat.com/brew/buildinfo?buildID=

# release number - to show what's new
REACT_APP_RELEASE_NUMBER_WHATS_NEW = 3

# run environment
REACT_APP_RUN_ENV=prod

REACT_APP_ART_DASH_SERVER_ROUTE=http://art-dash-server-art-build-dev.apps.ocp4.prod.psi.redhat.com

# art-dash server API endpoint
REACT_APP_API_ENDPOINT=api/v1

